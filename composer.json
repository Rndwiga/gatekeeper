{
    "name": "rndwiga/laravel-authentication",
    "type": "package",
    "description": "Authentication package for laravel projects.",
    "keywords": ["ui", "rndwiga",  "admin", "tyondo", "laravel"],
    "homepage": "https://github.com/Rndwiga/",
    "repositories": [
        {
            "type": "git",
            "reference":"master",
            "url": "https://github.com/Rndwiga/laravel-authentication.git"
        }
    ],
    "authors": [
        {
            "name": "Raphael Ndwiga",
            "email": "packages@tyondo.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "tyondo/menu-generator": "^1.0",
        "laravel/framework": "5.3.*|5.4.*|5.5.*|5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "autoload": {
        "psr-4": {
            "Rndwiga\\Authentication\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rndwiga\\Authentication\\Providers\\RndwigaAuthorizationServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "minimum-stability": "stable"

}

